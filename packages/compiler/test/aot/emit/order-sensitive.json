    "expect": {
      "targetCount": 1,
      "exprCount": 2,
      "instructions": [
        { "type": "propertyBinding", "target": 0, "to": "model", "mode": "toView" },
        { "type": "propertyBinding", "target": 0, "to": "checked", "mode": "twoWay" }
      ],
      "nested": []
    }
