{
  "name": "@aurelia-ls/build",
  "version": "0.2.0",
  "type": "module",
  "main": "./out/index.js",
  "types": "./out/index.d.ts",
  "exports": {
    ".": {
      "import": "./out/index.js",
      "types": "./out/index.d.ts"
    },
    "./vite": {
      "import": "./out/vite/index.js",
      "types": "./out/vite/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@aurelia-ls/compiler": "workspace:*",
    "@aurelia-ls/resolution": "workspace:*",
    "@aurelia-ls/transform": "workspace:*",
    "@aurelia/kernel": "file:../../aurelia/packages/kernel",
    "@aurelia/metadata": "file:../../aurelia/packages/metadata",
    "@aurelia/platform": "file:../../aurelia/packages/platform",
    "@aurelia/platform-browser": "file:../../aurelia/packages/platform-browser",
    "@aurelia/runtime": "file:../../aurelia/packages/runtime",
    "@aurelia/runtime-html": "file:../../aurelia/packages/runtime-html",
    "@aurelia/router": "file:../../aurelia/packages/router",
    "@aurelia/expression-parser": "file:../../aurelia/packages/expression-parser",
    "@aurelia/template-compiler": "file:../../aurelia/packages/template-compiler",
    "jsdom": "^25.0.1"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "vite": "^6.0.0"
  }
}
