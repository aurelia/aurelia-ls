{
  "au-repeat": {
    "prefix": "au-repeat",
    "body": [
      "<${1:div} repeat.for=\"${2:item} of ${3:items}\">",
      "\t$0",
      "</${1:div}>"
    ],
    "description": "Aurelia repeat.for template controller"
  },
  "au-repeat-index": {
    "prefix": "au-repeat-index",
    "body": [
      "<${1:div} repeat.for=\"${2:item} of ${3:items}\">",
      "\t<span>\\${\\$index + 1}:</span> \\${$2}$0",
      "</${1:div}>"
    ],
    "description": "Aurelia repeat.for with $index"
  },
  "au-if": {
    "prefix": "au-if",
    "body": [
      "<${1:div} if.bind=\"${2:condition}\">",
      "\t$0",
      "</${1:div}>"
    ],
    "description": "Aurelia if.bind template controller"
  },
  "au-else": {
    "prefix": "au-else",
    "body": [
      "<${1:div} else>",
      "\t$0",
      "</${1:div}>"
    ],
    "description": "Aurelia else template controller"
  },
  "au-show": {
    "prefix": "au-show",
    "body": [
      "<${1:div} show.bind=\"${2:condition}\">",
      "\t$0",
      "</${1:div}>"
    ],
    "description": "Aurelia show.bind (CSS visibility)"
  },
  "au-switch": {
    "prefix": "au-switch",
    "body": [
      "<${1:div} switch.bind=\"${2:value}\">",
      "\t<template case=\"${3:case1}\">$4</template>",
      "\t<template default-case>$0</template>",
      "</${1:div}>"
    ],
    "description": "Aurelia switch.bind with case/default-case"
  },
  "au-with": {
    "prefix": "au-with",
    "body": [
      "<${1:div} with.bind=\"${2:object}\">",
      "\t$0",
      "</${1:div}>"
    ],
    "description": "Aurelia with.bind scope change"
  },
  "au-portal": {
    "prefix": "au-portal",
    "body": [
      "<au-portal target=\"${1:body}\">",
      "\t$0",
      "</au-portal>"
    ],
    "description": "Aurelia portal for rendering to different DOM location"
  },
  "au-compose": {
    "prefix": "au-compose",
    "body": [
      "<au-compose component.bind=\"${1:componentName}\" model.bind=\"${2:data}\"></au-compose>$0"
    ],
    "description": "Aurelia dynamic composition"
  },
  "au-slot": {
    "prefix": "au-slot",
    "body": [
      "<au-slot name=\"${1:default}\">$0</au-slot>"
    ],
    "description": "Aurelia slot definition"
  },
  "au-slot-projection": {
    "prefix": "au-slot-projection",
    "body": [
      "<template au-slot=\"${1:default}\">",
      "\t$0",
      "</template>"
    ],
    "description": "Aurelia slot projection"
  },
  "au-promise": {
    "prefix": "au-promise",
    "body": [
      "<${1:div} promise.bind=\"${2:asyncOperation}\">",
      "\t<template pending>Loading...</template>",
      "\t<template then.from-view=\"${3:result}\">\\${$3}$0</template>",
      "\t<template catch.from-view=\"${4:error}\">Error: \\${$4.message}</template>",
      "</${1:div}>"
    ],
    "description": "Aurelia promise.bind with pending/then/catch"
  },
  "au-let": {
    "prefix": "au-let",
    "body": [
      "<let ${1:varName}.bind=\"${2:expression}\"></let>$0"
    ],
    "description": "Aurelia let element for local variables"
  },
  "au-bind": {
    "prefix": "au-bind",
    "body": [
      "${1:property}.bind=\"${2:expression}\"$0"
    ],
    "description": "Aurelia property binding"
  },
  "au-two-way": {
    "prefix": "au-two-way",
    "body": [
      "${1:value}.two-way=\"${2:property}\"$0"
    ],
    "description": "Aurelia explicit two-way binding"
  },
  "au-one-time": {
    "prefix": "au-one-time",
    "body": [
      "${1:property}.one-time=\"${2:expression}\"$0"
    ],
    "description": "Aurelia one-time binding (no observation)"
  },
  "au-from-view": {
    "prefix": "au-from-view",
    "body": [
      "${1:property}.from-view=\"${2:expression}\"$0"
    ],
    "description": "Aurelia from-view binding (view to model)"
  },
  "au-to-view": {
    "prefix": "au-to-view",
    "body": [
      "${1:property}.to-view=\"${2:expression}\"$0"
    ],
    "description": "Aurelia to-view binding (model to view)"
  },
  "au-trigger": {
    "prefix": "au-trigger",
    "body": [
      "${1:click}.trigger=\"${2:handler()}\"$0"
    ],
    "description": "Aurelia event trigger"
  },
  "au-delegate": {
    "prefix": "au-delegate",
    "body": [
      "${1:click}.delegate=\"${2:handler()}\"$0"
    ],
    "description": "Aurelia event delegation"
  },
  "au-capture": {
    "prefix": "au-capture",
    "body": [
      "${1:click}.capture=\"${2:handler()}\"$0"
    ],
    "description": "Aurelia event capture phase"
  },
  "au-ref": {
    "prefix": "au-ref",
    "body": [
      "ref=\"${1:elementRef}\"$0"
    ],
    "description": "Aurelia element reference"
  },
  "au-view-model-ref": {
    "prefix": "au-view-model-ref",
    "body": [
      "view-model.ref=\"${1:componentRef}\"$0"
    ],
    "description": "Aurelia component view-model reference"
  },
  "au-interpolation": {
    "prefix": "au-interpolation",
    "body": [
      "\\${$1}$0"
    ],
    "description": "Aurelia interpolation expression"
  },
  "au-class": {
    "prefix": "au-class",
    "body": [
      "class.bind=\"${1:cssClass}\"$0"
    ],
    "description": "Aurelia class binding"
  },
  "au-class-toggle": {
    "prefix": "au-class-toggle",
    "body": [
      "${1:active}.class=\"${2:condition}\"$0"
    ],
    "description": "Aurelia conditional class toggle"
  },
  "au-style": {
    "prefix": "au-style",
    "body": [
      "style.bind=\"${1:styleObject}\"$0"
    ],
    "description": "Aurelia style binding"
  },
  "au-style-prop": {
    "prefix": "au-style-prop",
    "body": [
      "style.${1:color}=\"${2:value}\"$0"
    ],
    "description": "Aurelia individual style property binding"
  },
  "au-custom-element": {
    "prefix": "au-custom-element",
    "body": [
      "<${1:my-component} ${2:prop}.bind=\"${3:value}\"></${1:my-component}>$0"
    ],
    "description": "Aurelia custom element usage"
  },
  "au-value-converter": {
    "prefix": "au-value-converter",
    "body": [
      "\\${$1 | $2$3}$0"
    ],
    "description": "Aurelia value converter (pipe)"
  },
  "au-binding-behavior": {
    "prefix": "au-binding-behavior",
    "body": [
      "${1:value}.bind=\"${2:expression} & ${3:behaviorName}\"$0"
    ],
    "description": "Aurelia binding behavior"
  }
}
