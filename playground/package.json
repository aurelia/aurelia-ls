{
  "name": "@aurelia-ls/playground",
  "private": true,
  "type": "module",
  "description": "E2E test fixtures for Aurelia Language Server",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "generate:golden": "cross-env GENERATE_EXPECTED=1 vitest run --test-name-pattern \"Layer 0\"",
    "generate:golden:debug": "cross-env AURELIA_DEBUG=lower,link,bind,typecheck,aot,overlay,ssr,transform,project AURELIA_TRACE=1 GENERATE_EXPECTED=1 vitest run --test-name-pattern \"Layer 0\"",
    "serve:golden:kitchen-sink-csr": "vite kitchen-sink-csr/golden --open",
    "serve:golden:issue-tracker": "vite issue-tracker/golden --open"
  },
  "devDependencies": {},
  "dependencies": {
    "aurelia": "*",
    "@aurelia/kernel": "*",
    "@aurelia/metadata": "*",
    "@aurelia/platform": "*",
    "@aurelia/platform-browser": "*",
    "@aurelia/runtime": "*",
    "@aurelia/runtime-html": "*",
    "@aurelia/expression-parser": "*",
    "@aurelia/template-compiler": "*",
    "@aurelia/router": "*",
    "@aurelia/route-recognizer": "*",
    "@aurelia/i18n": "*",
    "@aurelia-ls/compiler": "workspace:*",
    "@aurelia-ls/vite-plugin": "workspace:*",
    "@aurelia-ls/ssr": "workspace:*",
    "@aurelia-ls/transform": "workspace:*"
  }
}
