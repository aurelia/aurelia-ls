{
  "name": "router-app",
  "description": "Aurelia 2 Router SSR example",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:ssr": "node scripts/build-ssr.mjs",
    "build:ssg": "npm run build:ssr && node scripts/generate-static.mjs",
    "build:all": "npm run build:ssr && npm run build && npm run ssg",
    "ssg": "node scripts/generate-static.mjs",
    "preview": "npx serve dist"
  },
  "dependencies": {
    "aurelia": "*",
    "@aurelia/kernel": "*",
    "@aurelia/metadata": "*",
    "@aurelia/platform": "*",
    "@aurelia/platform-browser": "*",
    "@aurelia/runtime": "*",
    "@aurelia/runtime-html": "*",
    "@aurelia/expression-parser": "*",
    "@aurelia/template-compiler": "*",
    "@aurelia/router": "*",
    "@aurelia/route-recognizer": "*",
    "@aurelia-ls/build": "workspace:*"
  },
  "devDependencies": {
    "@aurelia/vite-plugin": "*",
    "@types/node": "^22.10.2",
    "esbuild": "^0.25.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.3"
  }
}
